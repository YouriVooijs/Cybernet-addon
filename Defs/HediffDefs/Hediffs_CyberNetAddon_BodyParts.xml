<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    
   
        <HediffDef ParentName="CNHediffProsthetic">
        <defName>CNNetworkedLung</defName>
        <label>Networked Lung</label>
        <labelNoun>A Networked Lung</labelNoun>
        <description>A Artificial lung that requires a network to function properly. If the connection with the network is lost, the lung will start to fail and weaken the host.</description>
        <spawnThingOnRemoved>CNNetworkedLung</spawnThingOnRemoved>
        <comps>
            <li Class="CyberNet.HediffCompPropertiesCyberNetImplant">
                <compClass>CyberNet.HediffCompCyberNetImplant</compClass>
                <DefaultSeverity>1</DefaultSeverity>
                <BandwidthUsage>0.20</BandwidthUsage>
                <ProcessorUsage>0.05</ProcessorUsage>
            </li>
        </comps>
        <stages>
            <li>
                <label>disconnected</label>
                <minSeverity>0</minSeverity>
                <capMods>
                    <li>
                        <partEfficiencyOffset>-0.3</partEfficiencyOffset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>throttled</label>
                <minSeverity>0.5</minSeverity>
                <capMods>
                    <li>
                        <partEfficiencyOffset>-0.10</partEfficiencyOffset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>sluggish</label>
                <minSeverity>0.75</minSeverity>
                <capMods>
                    <li>
                        <partEfficiencyOffset>0</partEfficiencyOffset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>connected</label>
                <minSeverity>1</minSeverity>
                <capMods>
                    <li>
                        <partEfficiencyOffset>0.10</partEfficiencyOffset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>overclocked</label>
                <minSeverity>1.15</minSeverity>
                <capMods>
                    <li>
                        <partEfficiencyOffset>0.20</partEfficiencyOffset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>supercharged</label>
                <minSeverity>1.3</minSeverity>
                <capMods>
                    <li>
                        <<partEfficiencyOffset>0.3</partEfficiencyOffset>
                    </li>
                </capMods>
            </li>
        </stages>
    </HediffDef>
</Defs>